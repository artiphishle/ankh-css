{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2016",
    "noUncheckedIndexedAccess": true
  },
  "include": ["lib/**/*.ts"],
  "exclude": ["node_modules"]
}
